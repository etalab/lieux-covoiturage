            "constraints":{
                "required":true
            }
        },
        {
            "name":"ad_lieu",
            "description":"",
            "example":"",
            "type":"string",
            "constraints":{
                "required":false
            }
        },
        {
            "name":"com_lieu",
            "description":"",
            "example":"",
            "type":"string",
            "constraints":{
                "required":false
            }
        },
        {
            "name":"code_insee",
            "description":"",
            "example":"",
            "type":"string",
            "constraints":{
                "required":true,
                "pattern":"^([013-9]\\d|2[AB1-9])\\d{3}$"
            }
        },
        {
            "name":"type_lieu",
            "description":"",
            "example":"",
            "type":"string",
            "constraints":{
                "required":true,
                "enum":[
                    "Aire de covoiturage",
                    "Sortie d'autoroute",
                    "Parking",
                    "Supermarché",
                    "Parking relais",
                    "Délaissé routier"
