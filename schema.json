    "fields":[
        {
            "name":"id_lieu",
            "description":"",
            "example":"35238-C-001",
            "type":"string",
            "constraints":{
                "required":true,
                "pattern":"^([013-9]\\d|2[AB1-9])\\d{3}-C-\\d{3}$"
            }
        },
        {
            "name":"nom_lieu",
            "description":"",
            "example":"Les Romains",
