        {
            "name":"ouverture",
            "description":"",
            "example":"",
            "type":"string",
            "constraints":{
                "required":false
            }
        },
        {
            "name":"amenageur",
            "description":"",
            "example":"",
            "type":"string",
            "constraints":{
                "required":false
            }
        },
        {
            "name":"eclairage",
            "description":"",
            "example":"",
            "type":"boolean",
            "constraints":{
                "required":false
            }
        },
        {
            "name":"commentaires",
            "description":"",
            "example":"",
            "type":"string",
            "constraints":{
                "required":false
