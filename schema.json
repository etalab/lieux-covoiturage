            "constraints":{
                "required":true
            }
        },
        {
            "name":"source",
            "description":"",
            "example":"",
            "type":"string",
            "constraints":{
                "required":true
            }
        },
        {
            "name":"Xlongitude",
            "description":"",
            "example":"",
            "type":"number",
            "constraints":{
                "required":true,
                "minimum":-180,
                "maximum":180
            }
        },
        {
            "name":"Ylatitude",
            "description":"",
            "example":"",
            "type":"number",
            "constraints":{
                "required":true,
                "minimum":-90,
                "maximum":90
            }
        },
        {
            "name":"nbre_places",
            "description":"",
